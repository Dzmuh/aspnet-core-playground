@page
@model DemoWebApplication.Pages.DataTypeAttributePageViewModel
@{
    ViewData["Title"] = "Home page";
}
<form id="demo" method="post">
    <div class="row">
        <div class="col">
            <div asp-validation-summary="All" class="text-danger"></div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="Input.DateTime"></label>
                <input asp-for="Input.DateTime" class="form-control" />
                <span asp-validation-for="Input.DateTime" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="Input.Date"></label>
                <input asp-for="Input.Date" class="form-control" />
                <span asp-validation-for="Input.Date" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="Input.Time"></label>
                <input asp-for="Input.Time" class="form-control" />
                <span asp-validation-for="Input.Time" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="Input.Time"></label>
                <input asp-for="Input.Time" class="form-control" />
                <span asp-validation-for="Input.Time" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label asp-for="Input.Duration"></label>
                <input asp-for="Input.Duration" class="form-control" />
                <span asp-validation-for="Input.Duration" class="text-danger"></span>
            </div>
        </div>
    </div>
</form>
